<Window x:Class="practical2WPF.ErrorWindow3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:practical2WPF"
        mc:Ignorable="d"
        Title="ErrorWindow3" Height="145" Width="1000"  WindowStyle="None"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel HorizontalAlignment="Left" Height="52" Margin="6,0,0,0" VerticalAlignment="Top" Width="328">
            <Label Content="Warnning" HorizontalAlignment="Left" Height="42" Width="328" FontFamily="Segoe UI Historic" FontSize="20"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="52" Margin="6,27,0,0" VerticalAlignment="Top" Width="472">
            <Label Content="Are you sure to Log Out?" HorizontalAlignment="Left" Height="58" Width="472" FontFamily="Nirmala UI" FontWeight="Bold" FontSize="36"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="87" Margin="721,27,0,0" VerticalAlignment="Top" Width="236">
            <Button Background="Green" Foreground="White" Content="Yes" FontSize="12" HorizontalAlignment="Left" Height="45" Width="236" Click="Button_Click"/>
            <Button Background="Red" Foreground="White" Content="No" FontSize="12" HorizontalAlignment="Left" Height="45" Width="236" Click="Button_Click_1"/>
        </StackPanel>

    </Grid>
</Window>
