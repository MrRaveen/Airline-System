<UserControl x:Class="practical2WPF.PaymentPart"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:practical2WPF"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Background="White">
    <Grid>
        <StackPanel HorizontalAlignment="Left" Height="54" Margin="31,10,0,0" VerticalAlignment="Top" Width="320">
            <TextBlock HorizontalAlignment="Left" Height="44" TextWrapping="Wrap" Text="Make your payment" Width="310" FontFamily="Nirmala UI Semilight" FontSize="22"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="35" Margin="31,69,0,0" VerticalAlignment="Top" Width="155">
            <Label Content="Payment currency" HorizontalAlignment="Left" Height="29" Width="136" FontFamily="Nirmala UI" FontWeight="Bold" FontSize="14"/>
        </StackPanel>

        <StackPanel HorizontalAlignment="Left" Height="38" Margin="186,69,0,0" VerticalAlignment="Top" Width="214">
            <ComboBox Name="combo1" HorizontalAlignment="Left" Height="34" VerticalAlignment="Top" Width="203" SelectionChanged="Combo1_SelectionChanged"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="36" Margin="31,124,0,0" VerticalAlignment="Top" Width="296">
            <Label Content="Card type (Only these cards are available)" HorizontalAlignment="Left" Height="29" Width="286" FontFamily="Nirmala UI" FontWeight="Bold" FontSize="14"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="59" Margin="31,165,0,0" VerticalAlignment="Top" Width="94">
            <Border BorderBrush="White" BorderThickness="1" HorizontalAlignment="Left" Height="58" VerticalAlignment="Top" Width="92">
                <Border.Background>
                    <ImageBrush ImageSource="/Images/card1.jpg">
                    </ImageBrush>
                </Border.Background>
            </Border>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="59" Margin="130,165,0,0" VerticalAlignment="Top" Width="98">
            <Border BorderBrush="White" BorderThickness="1" HorizontalAlignment="Left" Height="62" VerticalAlignment="Top" Width="92">
                <Border.Background>
                    <ImageBrush ImageSource="/Images/card2.jpg">
                    </ImageBrush>
                </Border.Background>
            </Border>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="59" Margin="233,165,0,0" VerticalAlignment="Top" Width="98">
            <Border BorderBrush="White" BorderThickness="1" HorizontalAlignment="Left" Height="62" VerticalAlignment="Top" Width="92">
                <Border.Background>
                    <ImageBrush ImageSource="Images/card3.jpg"/>
                </Border.Background>
            </Border>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="59" Margin="336,165,0,0" VerticalAlignment="Top" Width="98">
            <Border BorderBrush="White" BorderThickness="1" HorizontalAlignment="Left" Height="62" VerticalAlignment="Top" Width="92">
                <Border.Background>
                    <ImageBrush ImageSource="Images/card4.jpg"/>
                </Border.Background>
            </Border>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="59" Margin="439,165,0,0" VerticalAlignment="Top" Width="98">
            <Border BorderBrush="White" BorderThickness="1" HorizontalAlignment="Left" Height="62" VerticalAlignment="Top" Width="92">
                <Border.Background>
                    <ImageBrush ImageSource="Images/card5.jpg"/>
                </Border.Background>
            </Border>
        </StackPanel>
        <Border BorderBrush="White" Background="DarkBlue" BorderThickness="1" HorizontalAlignment="Left" Height="581" Margin="703,0,0,-131" VerticalAlignment="Top" Width="97"/>
        <StackPanel HorizontalAlignment="Left" Height="35" Margin="31,300,0,0" VerticalAlignment="Top" Width="121">
            <Label Content="Card Number" HorizontalAlignment="Left" Height="29" Width="108" FontFamily="Nirmala UI" FontWeight="Bold" FontSize="14"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="53" Margin="186,304,0,0" VerticalAlignment="Top" Width="214">
            <TextBox Name="box1" HorizontalAlignment="Left" Height="32" TextWrapping="Wrap" Text="" Width="214"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="35" Margin="31,348,0,0" VerticalAlignment="Top" Width="121">
            <Label Content="Expiry Date" HorizontalAlignment="Left" Height="29" Width="108" FontFamily="Nirmala UI" FontWeight="Bold" FontSize="14"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="51" Margin="186,352,0,0" VerticalAlignment="Top" Width="214">
            <DatePicker Name="picker1" HorizontalAlignment="Left" Height="34" Width="214"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="35" Margin="381,405,0,0" VerticalAlignment="Top" Width="121">
            <Label Content="Holder phone" HorizontalAlignment="Left" Height="29" Width="108" FontFamily="Nirmala UI" FontWeight="Bold" FontSize="14"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="35" Margin="31,405,0,0" VerticalAlignment="Top" Width="121">
            <Label Content="Card holder mail" HorizontalAlignment="Left" Height="29" Width="124" FontFamily="Nirmala UI" FontWeight="Bold" FontSize="14"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="42" Margin="186,408,0,0" VerticalAlignment="Top" Width="190">
            <TextBox Name="box2" HorizontalAlignment="Left" Height="32" TextWrapping="Wrap" Text="" Width="182"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="42" Margin="484,405,0,0" VerticalAlignment="Top" Width="152">
            <TextBox Name="box5" HorizontalAlignment="Left" Height="32" TextWrapping="Wrap" Text="" Width="142"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="35" Margin="31,445,0,-30" VerticalAlignment="Top" Width="121">
            <Label Content="Postal Code" HorizontalAlignment="Left" Height="29" Width="124" FontFamily="Nirmala UI" FontWeight="Bold" FontSize="14"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="42" Margin="186,455,0,-47" VerticalAlignment="Top" Width="141">
            <TextBox Name="box3" HorizontalAlignment="Left" Height="32" TextWrapping="Wrap" Text="" Width="124"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="35" Margin="383,445,0,-30" VerticalAlignment="Top" Width="96">
            <Label Content="City living" HorizontalAlignment="Left" Height="29" Width="86" FontFamily="Nirmala UI" FontWeight="Bold" FontSize="14"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="42" Margin="484,455,0,-47" VerticalAlignment="Top" Width="152">
            <TextBox Name="box6" HorizontalAlignment="Left" Height="32" TextWrapping="Wrap" Text="" Width="142"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="35" Margin="31,485,0,-70" VerticalAlignment="Top" Width="121">
            <Label Content="Country" HorizontalAlignment="Left" Height="29" Width="124" FontFamily="Nirmala UI" FontWeight="Bold" FontSize="14"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="42" Margin="186,497,0,-89" VerticalAlignment="Top" Width="141">
            <TextBox Name="box4" HorizontalAlignment="Left" Height="32" TextWrapping="Wrap" Text="" Width="124"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="37" Margin="484,502,0,-89" VerticalAlignment="Top" Width="152">
            <Button Content="Proceed" Foreground="White" Background="Indigo" HorizontalAlignment="Left" Height="33" Width="142" Click="Button_Click"/>
        </StackPanel>
        <StackPanel HorizontalAlignment="Left" Height="38" Margin="31,242,0,0" VerticalAlignment="Top" Width="214">
            <ComboBox Name="combo2" HorizontalAlignment="Left" Height="34" VerticalAlignment="Top" Width="203" SelectionChanged="Combo2_SelectionChanged"/>
        </StackPanel>

    </Grid>
</UserControl>
